<!DOCTYPE html>
@{
    ViewBag.Title = "General";
}
<style>
    html, body {
        margin: 0px !important;
        padding: 0px !important;
        overflow-x: hidden;
    }



    @@media screen and (max-width: 767px) {
        .sidenav {
            height: auto;
            padding: 15px;
        }

        .row.content {
            height: auto;
        }
